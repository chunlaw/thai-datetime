{
  "name": "thai-datetime",
  "version": "1.0.1",
  "description": "Package to give datetime in Thai",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "repository": "https://github.com/chunlaw/thai-datetime",
  "author": "Chun Law",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "prebuild": "yarn clean && yarn test",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:cjs": "tsc --target es2015 --module commonjs --outDir dist",
    "build": "yarn build:cjs && yarn build:esm",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "keywords": [
    "thai",
    "thailand",
    "datetime",
    "time",
    "date"
  ],
  "homepage": "https://ink.chunlaw.io/ref/datetime"
}
